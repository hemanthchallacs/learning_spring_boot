package com.elearning.spring.repo;



import org.springframework.boot.autoconfigure.data.web.SpringDataWebProperties.Pageable;
import org.springframework.data.domain.Page;
import org.springframework.data.jpa.repository.JpaRepository;

import com.elearning.spring.entities.Enrolled;
import com.elearning.spring.entities.User;

public interface EnrolledRepository extends JpaRepository<Enrolled, String> {
	Page<Enrolled> findByUser(User user_id, Pageable pageable);
}
